<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <classPathEntry location="F:\Java\JAFrameWorkLayer\Plugs\mybatis-generator\lib\mysql-connector-java-5.1.42-bin.jar" />

  <context id="DB2Tables" targetRuntime="MyBatis3">
  
	 <commentGenerator>
		<!--  关闭自动生成的注释  -->
		<property name="suppressAllComments" value="true" />
	</commentGenerator>

    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
        connectionURL="jdbc:mysql://192.168.0.118:3306/AFData"
        userId="root"
        password="123456">
    </jdbcConnection>
	
    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

    <javaModelGenerator targetPackage="com.domain.users" targetProject="F:\Java\JAFrameWorkLayer\Plugs\mybatis-generator\lib">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="com.mapping.userMappers"  targetProject="F:\Java\JAFrameWorkLayer\Plugs\mybatis-generator\lib">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER" targetPackage="com.repository.users"  targetProject="F:\Java\JAFrameWorkLayer\Plugs\mybatis-generator\lib">
      <property name="enableSubPackages" value="true" />
    </javaClientGenerator>

    <table schema="AFData" tableName="user" domainObjectName="User"  enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"  enableSelectByExample="false"  selectByExampleQueryId="false" >
      <property name="useActualColumnNames" value="true"/>
      <generatedKey column="id" sqlStatement="DB2" identity="true" />
	  <columnOverride column="last_active_time" property="lastActiveTime" javaType="Timestamp" />
    </table>
	
	<table schema="AFData" tableName="role" domainObjectName="Role"  enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"  enableSelectByExample="false"  selectByExampleQueryId="false" >
      <property name="useActualColumnNames" value="true"/>
      <generatedKey column="id" sqlStatement="DB2" identity="true" />
	  <columnOverride column="system_code" property="systemCode"  />
    </table>

  </context>
</generatorConfiguration>
